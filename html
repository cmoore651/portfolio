            $(document).ready(function () {



                    particlesJS('full-screen', {
                        particles: {
                            color: '#FF458F',
                            color_random: false,
                            shape: 'circle', // "circle", "edge" or "triangle"
                        opacity: {
                            opacity: .5,
                            anim: {
                                enable: false,
                                speed: 1,
                                opacity_min: 0,
                                sync: false
                            }
                        },
                        size: 3.5,
                        size_random: true,
                        nb: 50,
                        line_linked: {
                            enable_auto: true, // SHOULD BE TRUE //
                            distance: 500,
                            color: '#FF458F',
                            opacity: .5,
                            width: 1,
                            condensed_mode: {
                                enable: false,
                                rotateX: 600,
                                rotateY: 600
                            }
                        },
                            anim: {
                                enable: true,
                                speed: 1.5
                            }
                        },
                        interactivity: {
                            enable: true,
                            mouse: {
                                distance: 250
                            },
                        detect_on: 'window', // "canvas" or "window"
                        mode: 'grab', // "grab" of false
                        line_linked: {
                            opacity: .5
                        },
                        events: {
                            onclick: {
                                enable: true,
                                mode: 'push', // "push" or "remove"
                                nb: 1
                            },
                            onresize: {
                                enable: true,
                                mode: 'bounce', // "out" or "bounce"
                                density_auto: false,
                                density_area: 80 // nb_particles = particles.nb * (canvas width *  canvas height / 1000) / density_area
                            }
                        }
                        },
                      // Retina Display Support //
                      retina_detect: true
                    });





            });
        </script>




